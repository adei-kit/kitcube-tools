; kitcube.ini
; A. Kopmann
;
;

; Data server for simulation of device data
[DataServer]
module = Mast12-calc
;module = Mast01
;module = Simuation
;module = Ceilometer

; Reading data and filling in the database
[Reader]
;module = Mast12-calc
module = Mast11-das Mast11-dal Mast11-dar
;samplingTime = 1 min
samplingTime = 5 sec


; Global parameters for the whole setup related to the
; database connection.
[Common]
configDir = /home/cube/kitcube-tools-0.6/etc
dataDir = /home/cube/data
remoteDir = /home/cube/data
archiveDir = /home/cube/archive
dbHost = 192.168.1.102
dbName = kitcube_actual
dbUser = cube
dbPassword = cube
rsyncArgs = --append
 
[Axis]
axis = DA DC DN E FC FH FM FV GI H KD KH KM KMS LA MA MR MS N NE NF NO PA QR R T TA V VD X XR Z
DA_name = Density_(Air)
DA_unit = kg/m^3
DC_name = Density_(CO2)
DC_unit = mg/m^3
DN_name = Spectral_Number_Density
DN_unit = mm^3/m^3
E_name = Voltage
E_unit = V
FC_name = CO2_flow
FC_unit = mg/m^2*s
FH_name = Kinematic_heat_flow
FH_unit = K*m/s
FM_name = Humidity_flow
FM_unit = g/m^2*s
FV_name = Covariance
FV_unit = m^2/s^2
GI_name = Inklination
GI_unit = grad
H_name = Heigth
H_unit = m
KD_name = Duration
KD_unit = s
KH_name = Duration
KH_unit = h
KM_name = Duration
KM_unit = min
KMS_name = Duration
KMS_unit = ms
LA_name = Lambda
LA_unit = 1/mm
MA_name = Absolute_Humidity
MA_unit = g/m^3
MR_name = Relative_Humidity
MR_unit = %
MS_name = Specific_humidity
MS_unit = g/kg
N_name = Percipitation
N_unit = mm
NE_name = Percipitation_rate
NE_unit = mm/s
NF_name = Percipitation_rate
NF_unit = mm/h
NO_name = No_Unit
NO_unit = 
PA_name = Air_pressure
PA_unit = hPa
QR_name = Quality
QR_unit = %
R_name = Radiation
R_unit = W/m^2
T_name = Temperature
T_unit = C
T_range = -50 100
TA_name = Temperature
TA_unit = K
TA_range = 0 400
V_name = Wind_speed
V_unit = m/s
VD_name = Wind_direction
VD_unit = grad
X_name = Data
X_unit = Byte
XR_name = Data
XR_unit = %
Z_name = Reflectivity
Z_unit = dB

; First letters: (See sensor naming convention)
;

; Second letters: 
; A Absolute, R Reletive, S Specific
; H Heat
; I Inclination
; D Wind direction, A Air/Wind, C CO2
; F Rate


;
; Definition of modules that can be used by the module parameter of the applications
;


; Energie Bilanz Mast 1

; Sampling rate 20Hz
[Mast11-das]
moduleName = EB1
moduleNumber = 11
moduleType = Mast
moduleComment = Energy_balance_station_1
sensorGroup = DAS
datafileMask = messwiese<index>.DAS
sensorList = EB1.DAS.sensors
datafileTemplate = M11_01.DAS.template
samplingTime = 0.05 sec
remoteDir = pc-eb1:/cygdrive/d

; Sampling rate 1Hz
[Mast11-dal]
moduleName = EB1
moduleNumber = 11
moduleType = Mast
moduleComment = Energy_balance_station_1
sensorGroup = DAL
datafileMask = messwiese<index>.DAL
sensorList = EB1.DAL.sensors
datafileTemplate = M11_01.DAL.template
samplingTime = 1 sec
remoteDir = pc-eb1:/cygdrive/d

; Mean data of 10min intervals
[Mast11-dar]
moduleName = EB1
moduleNumber = 11
moduleType = Mast
moduleComment = Energy_balance_station_1
sensorGroup = DAR
datafileMask = messwiese<index>.DAR
sensorList = EB1.DAR.sensors
datafileTemplate = M11_01.DAR.template
samplingTime = 10 min
remoteDir = pc-eb1:/cygdrive/d

; Caclulated values also in a 10 min interval
[Mast11-dax]
moduleName = EB1
moduleNumber = 11
moduleType = Mast
moduleComment = Energy_balance_station_1
sensorGroup = DAX
datafileMask = messwiese<index>.DAX
sensorList = EB1.DAX.sensors
datafileTemplate = M11_01.DAX.template
samplingTime = 10 min
remoteDir = pc-eb1:/cygdrive/d/


